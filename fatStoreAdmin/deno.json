{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
  },
  "tasks": {
    "dev": "deno run --watch --allow-net --env-file --allow-env --allow-read main.ts",
    "test": "deno test --fail-fast --coverage",
    "snap-test": "deno run test --allow-read --allow-write -- --snap",
    "snap-test-update": "deno run snap-test --update"
  },
  "imports": {
    "@felix/argon2": "jsr:@felix/argon2@^2.0.2",
    "@sinclair/typebox": "npm:@sinclair/typebox@^0.34.31",
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.10",
    "ajv": "npm:ajv@^8.17.1",
    "ajv-formats": "npm:ajv-formats@^3.0.1",
    "fast-jwt": "npm:fast-jwt@^6.0.1",
    "mysql2": "npm:mysql2@^3.14.0/promise",
    "oak": "https://deno.land/x/oak@v17.1.3/mod.ts",
    "oak-response": "https://deno.land/x/oak@v17.1.3/response.ts"
  }
}
